<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <SelectDialog
        id="ticketIdValueHelpDialog"
        title="Tickets"
        items="{/TicketsCollection}"
        search="_handleValueHelpSearch"
        confirm="_handleValueHelpClose"
        cancel="_handleValueHelpClose"
        multiSelect="true">
        <StandardListItem
            iconDensityAware="false"
            iconInset="false"
            title="{Id}"
            description="{Title}" />
    </SelectDialog>
</core:FragmentDefinition>