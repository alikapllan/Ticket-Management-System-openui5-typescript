<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <SelectDialog
        id="ticketIdValueHelpDialog"
        title="Tickets"
        items="{ticketModel>/}"
        search="_onTicketIdValueHelpSearch"
        confirm="_onTicketIdValueHelpClose"
        cancel="_onTicketIdValueHelpClose"
        multiSelect="true">
        <StandardListItem
            iconDensityAware="false"
            iconInset="false"
            title="{ticketModel>ticketId}"
            description="{ticketModel>title}" />
    </SelectDialog>
</core:FragmentDefinition>