<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <SelectDialog
        id="AssignedToValueHelpDialog"
        title="Team Members"
        items="{/TeamMemberCollection}"
        search=".onValueHelpSearchAssignedTo"
        confirm=".onValueHelpCloseAssignedTo"
        cancel=".onValueHelpCloseAssignedTo">
        <StandardListItem
            iconDensityAware="false"
            iconInset="false"
            title="{Name}"
            description="{Role}" />
    </SelectDialog>
</core:FragmentDefinition>