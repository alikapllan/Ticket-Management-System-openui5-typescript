<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <SelectDialog
        id="AssignedToValueHelpDialog"
        title="{i18n>teamMembersTitleValueHelp}"
        items="{teamMemberModel>/}"
        search=".onValueHelpSearchAssignedTo"
        confirm=".onValueHelpCloseAssignedTo"
        cancel=".onValueHelpCloseAssignedTo">
        <StandardListItem
            iconDensityAware="false"
            iconInset="false"
            title="{teamMemberModel>fullName}"
            description="{teamMemberModel>role}" />
    </SelectDialog>
</core:FragmentDefinition>