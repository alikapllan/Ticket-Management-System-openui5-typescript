{
  "compilerOptions": {
    "target": "ES2023",
    "module": "ES2022",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "strict": false,
    "strictPropertyInitialization": false,
    "rootDir": "tmui5",
    "baseUrl": "./",
    "types": ["@sapui5/types"],
    "paths": {
      "tmui5/*": ["webapp/*"]
    },
    "forceConsistentCasingInFileNames": true
  },
  "include": ["./webapp/**/*"]
}
